submit_for_review true
force true
skip_screenshots true
release_notes({
  'default' => ENV["RELEASE_NOTES"],
  'en-US' => ENV["RELEASE_NOTES"]
})
run_precheck_before_submit false

for_platform :mac do
  platform "osx"
  pkg "#{ENV['PROJECT_NAME']}.pkg"
end
