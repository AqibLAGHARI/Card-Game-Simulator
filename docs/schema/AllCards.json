{
	"$schema": "http://json-schema.org/schema#",
	"id": "https://cardgamesim.finoldigital.com/schemas/AllCards.json",
	"description": "You may choose to not have an AllCards.json, since you may instead define all the card information directly in AllSets.json.",
	"items": {
		"title": "Card",
		"type": "object",
		"properties": {
			"!CardGameDef.json#/properties/cardIdIdentifier": {
				"type": "string"
			},
			"!CardGameDef.json#/properties/cardNameIdentifier": {
				"type": "string"
			},
			"!CardGameDef.json#/properties/cardSetIdentifier": {
				"type": "string"
			}
		},
		"additionalProperties": {
			"description": "!CardGameDef.json#/properties/cardProperties"
		},
		"required": ["!CardGameDef.json#/properties/cardIdIdentifier"]
	},
	"type": "array"
}
